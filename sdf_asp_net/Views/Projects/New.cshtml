@model sdf_asp_net.Models.Project
@{
    ViewBag.Title = "New";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<fieldset>
    <h1>Neues Projekt</h1>
    <form id="form1" method="post" action="@Url.Action("Create", "Projects")"
          enctype="application/x-www-form-urlencoded">
        <br />
        <div class="form-group">
            <div class="col-md-10">
                <div>
                    <label for="Name">Projektname:</label>
                </div>
                <br />
                <div>
                    <input name="Name" type="text" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>
        </div>
    </form>
</fieldset>

